// Rock Puzzle - First Playthrough
ds_map_add(global.text_data_en, "SCR_TEXT_PATIENCE_133", "* Looks like the spikes shot up./")
ds_map_add(global.text_data_en, "SCR_TEXT_PATIENCE_134", "* It’s pretty easy though. Just push this rock and-%")
ds_map_add(global.text_data_en, "SCR_TEXT_PATIENCE_135", "* Now you just wait one gosh darn second, pardner!")
ds_map_add(global.text_data_en, "SCR_TEXT_PATIENCE_136", "* Who said you could go pushin’ me around?/")
ds_map_add(global.text_data_en, "SCR_TEXT_PATIENCE_137", "* (*Sigh* This again...)/")
ds_map_add(global.text_data_en, "SCR_TEXT_PATIENCE_138", "* Yes, hello. We just want to get through./")
ds_map_add(global.text_data_en, "SCR_TEXT_PATIENCE_139", "* That pushin’ trick ain’t gonna work, hon!/")
ds_map_add(global.text_data_en, "SCR_TEXT_PATIENCE_140", "* (...Mind giving me a hand, kid?)/")
// Spared/Ran from Whimsun
ds_map_add(global.text_data_en, "SCR_TEXT_PATIENCE_141", "* I kindly explain to the rock I need to go home./")
ds_map_add(global.text_data_en, "SCR_TEXT_PATIENCE_142", "* So if it moved, it’d help a lot./")
ds_map_add(global.text_data_en, "SCR_TEXT_PATIENCE_143", "* Now THAT'S what I'm talkin' about! Pure patience!/")
ds_map_add(global.text_data_en, "SCR_TEXT_PATIENCE_144", "* Go on and push me, hon./")
ds_map_add(global.text_data_en, "SCR_TEXT_PATIENCE_145", "* Huh. What’da know?/")
ds_map_add(global.text_data_en, "SCR_TEXT_PATIENCE_146", "* You handled that way better than I could./")
// Scared Whimsun
ds_map_add(global.text_data_en, "SCR_TEXT_PATIENCE_147", "* I do my best scary face to FORCE the rock to move!/")
ds_map_add(global.text_data_en, "SCR_TEXT_PATIENCE_148", "* ...But I feel way too bad and apologize profusely./")
ds_map_add(global.text_data_en, "SCR_TEXT_PATIENCE_149", "* Well, since you apologized so nicely.../")
ds_map_add(global.text_data_en, "SCR_TEXT_PATIENCE_150", "* Might as well let ya push me./")
ds_map_add(global.text_data_en, "SCR_TEXT_PATIENCE_151", "* ...Huh. That works, I guess./")
// Killed Whimsun
ds_map_add(global.text_data_en, "SCR_TEXT_PATIENCE_152", "* I try to scare the rock into moving./")
ds_map_add(global.text_data_en, "SCR_TEXT_PATIENCE_153", "* It’ll take more than that to move ME, sweetie./")
ds_map_add(global.text_data_en, "SCR_TEXT_PATIENCE_154", "* I threaten the rock into moving./")
ds_map_add(global.text_data_en, "SCR_TEXT_PATIENCE_155", "* .../")
ds_map_add(global.text_data_en, "SCR_TEXT_PATIENCE_156", "* I try to push the rock-%")
ds_map_add(global.text_data_en, "SCR_TEXT_PATIENCE_157", "* Woah, woah! Stop right there!/")
ds_map_add(global.text_data_en, "SCR_TEXT_PATIENCE_158", "* You can... push me.../")
ds_map_add(global.text_data_en, "SCR_TEXT_PATIENCE_159", "* A little too much, don’t you think?/")
ds_map_add(global.text_data_en, "SCR_TEXT_PATIENCE_160", "* ...?/")
ds_map_add(global.text_data_en, "SCR_TEXT_PATIENCE_161", "* Well... Uh.../")
//
ds_map_add(global.text_data_en, "SCR_TEXT_PATIENCE_162", "* Let’s continue. After you.%%/")

// Entering the Tunnels - First Playthrough
ds_map_add(global.text_data_en, "SCR_TEXT_PATIENCE_163", "* Hey, don't sweat it./")
ds_map_add(global.text_data_en, "SCR_TEXT_PATIENCE_164", "* There’s a ladder that’ll take us to the next room./")
ds_map_add(global.text_data_en, "SCR_TEXT_PATIENCE_165", "* But we’ll have to jump down to get to it./")
ds_map_add(global.text_data_en, "SCR_TEXT_PATIENCE_166", "* Huh? You want me to... CARRY you down?/")
ds_map_add(global.text_data_en, "SCR_TEXT_PATIENCE_167", "* ...I guess I could try./")
ds_map_add(global.text_data_en, "SCR_TEXT_PATIENCE_168", "* There. Wasn’t a rough landing, was it?/")
ds_map_add(global.text_data_en, "SCR_TEXT_PATIENCE_169", "* Alright, cool. Let’s go./")
ds_map_add(global.text_data_en, "SCR_TEXT_PATIENCE_170", "* It... shrunk?/")
ds_map_add(global.text_data_en, "SCR_TEXT_PATIENCE_171", "* I swear it was bigger two weeks ago./")
// Whimsun is alive
ds_map_add(global.text_data_en, "SCR_TEXT_PATIENCE_172", "* Awww man... I also fell down here by accident.../")
ds_map_add(global.text_data_en, "SCR_TEXT_PATIENCE_173", "* Now we’re all stuck here... *Sniff*...!/")
ds_map_add(global.text_data_en, "SCR_TEXT_PATIENCE_174", "*  Uh-huh... You know you can fly, right?/")
ds_map_add(global.text_data_en, "SCR_TEXT_PATIENCE_175", "* O-oh. I forgot... *Sniff*./")
ds_map_add(global.text_data_en, "SCR_TEXT_PATIENCE_176", "* S-sorry for leaving you out to dry like this.../")
ds_map_add(global.text_data_en, "SCR_TEXT_PATIENCE_177", "* But I have a doctor's appointment.../")
ds_map_add(global.text_data_en, "SCR_TEXT_PATIENCE_178", "* ...15 minutes ago. Awww man.../")
//
ds_map_add(global.text_data_en, "SCR_TEXT_PATIENCE_179", "* ...Guess we gotta find a way out of here./")
ds_map_add(global.text_data_en, "SCR_TEXT_PATIENCE_180", "* Hm... Oh, here’s an idea./")
ds_map_add(global.text_data_en, "SCR_TEXT_PATIENCE_181", "* This entrance leads to the Tunnels. /")
ds_map_add(global.text_data_en, "SCR_TEXT_PATIENCE_182", "* They were built alongside the RUINS./")
ds_map_add(global.text_data_en, "SCR_TEXT_PATIENCE_183", "* And served as fast-travel to the City, HOME./")
ds_map_add(global.text_data_en, "SCR_TEXT_PATIENCE_184", "* It’s our best bet out of here, so let’s-%")
ds_map_add(global.text_data_en, "SCR_TEXT_PATIENCE_185", "* Ha! Really thought you could hide from ME?!/")
ds_map_add(global.text_data_en, "SCR_TEXT_PATIENCE_186", "* Well, THINK AGAIN! I'm known for many things.../")
ds_map_add(global.text_data_en, "SCR_TEXT_PATIENCE_187", "* And sneaking is ONE of them! Sabotaging TOO! So.../")
ds_map_add(global.text_data_en, "SCR_TEXT_PATIENCE_188", "* Prepare to face the Underground’s Mightiest Detective.../")
ds_map_add(global.text_data_en, "SCR_TEXT_PATIENCE_189", "* ...Who shall follow you EVERYWHERE YOU GO!/")
ds_map_add(global.text_data_en, "SCR_TEXT_PATIENCE_190", "* *Sigh*... Don’t worry, kid./")
ds_map_add(global.text_data_en, "SCR_TEXT_PATIENCE_191", "* I won’t let that moron go anywhere near you./")
ds_map_add(global.text_data_en, "SCR_TEXT_PATIENCE_192", "* If he tries, well.../") 
ds_map_add(global.text_data_en, "SCR_TEXT_PATIENCE_193", "* I’ll just throw ‘em half across the planet./")

// Broken Bridge - First Playthrough
ds_map_add(global.text_data_en, "SCR_TEXT_PATIENCE_194", "* Ah, come on... Are you serious?/")
ds_map_add(global.text_data_en, "SCR_TEXT_PATIENCE_195", "* (This better be the last roadblock, I swear...)/")
ds_map_add(global.text_data_en, "SCR_TEXT_PATIENCE_196", "* Alright, so... the bridge gave out./")
ds_map_add(global.text_data_en, "SCR_TEXT_PATIENCE_197", "* I was here earlier and the bridge was up./")
ds_map_add(global.text_data_en, "SCR_TEXT_PATIENCE_198", "* So this is very recent, which means.../")
ds_map_add(global.text_data_en, "SCR_TEXT_PATIENCE_199", "* The pieces might still be around here./")
ds_map_add(global.text_data_en, "SCR_TEXT_PATIENCE_200", "* Mind helping me look for them?/")
ds_map_add(global.text_data_en, "SCR_TEXT_PATIENCE_201", "* That should be all of them. Way to go, kid./")
ds_map_add(global.text_data_en, "SCR_TEXT_PATIENCE_202", "* Let’s bring ‘em back to the bridge./")

// Heartstrong & Co-workers
ds_map_add(global.text_data_en, "SCR_TEXT_PATIENCE_203", "* Heartstrong! There you are!/")
ds_map_add(global.text_data_en, "SCR_TEXT_PATIENCE_204", "* Oh, hey guys. What's up?/")
ds_map_add(global.text_data_en, "SCR_TEXT_PATIENCE_205", "* It’s horrible!/")
ds_map_add(global.text_data_en, "SCR_TEXT_PATIENCE_206", "* Woah, what happened?/")
ds_map_add(global.text_data_en, "SCR_TEXT_PATIENCE_207", "* *Huff* That Harold guy stormed in, and.../")
ds_map_add(global.text_data_en, "SCR_TEXT_PATIENCE_209", "* Crashed into a wall causing a HUGE landslide!/")
ds_map_add(global.text_data_en, "SCR_TEXT_PATIENCE_210", "* What?!/")
ds_map_add(global.text_data_en, "SCR_TEXT_PATIENCE_211", "* (Ugh, I knew he’d cause trouble...)/")
ds_map_add(global.text_data_en, "SCR_TEXT_PATIENCE_212", "* We need your help!/")
ds_map_add(global.text_data_en, "SCR_TEXT_PATIENCE_213", "* Wait, what about the kid? I can’t just-/")
ds_map_add(global.text_data_en, "SCR_TEXT_PATIENCE_214", "* H-hey!/")
ds_map_add(global.text_data_en, "SCR_TEXT_PATIENCE_215", "* That can wait!/")
ds_map_add(global.text_data_en, "SCR_TEXT_PATIENCE_216", "* Come on!/")
ds_map_add(global.text_data_en, "SCR_TEXT_PATIENCE_217", "* Sorry, kid! I'll try to be back soon!")
ds_map_add(global.text_data_en, "SCR_TEXT_PATIENCE_218", "* You’ll have to fend for yourself a bit!")
ds_map_add(global.text_data_en, "SCR_TEXT_PATIENCE_219", "* Ow- My head!/")







