ds_map_add(global.text_data_en, "SCR_TEXT_3316", "YOU WERE TRYING&TO PROTECT&YOURSELF.../")
ds_map_add(global.text_data_en, "SCR_TEXT_3317", "WHILE MAKING IT&SO I DIDN'T&HAVE TO LIE!/")
ds_map_add(global.text_data_en, "SCR_TEXT_3318", "I PICKED UP ON&THIS, AND FOLLOWED&YOUR PLAN./")
