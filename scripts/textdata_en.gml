ds_map_add(global.text_data_en, "SCR_TEXT_3240", "HELLO^1!&THIS IS PAPYRUS!!!/")
ds_map_add(global.text_data_en, "SCR_TEXT_3241", "REMEMBER WHEN&I ASKED YOU&ABOUT CLOTHES?/")
