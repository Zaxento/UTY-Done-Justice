ds_map_add(global.text_data_en, "SCR_TEXT_3340", "I WAS JUST&THINKING.../")
ds_map_add(global.text_data_en, "SCR_TEXT_3341", "YOU^1, ME^1, AND&UNDYNE SHOULD ALL&HANG OUT SOMETIME!/")
