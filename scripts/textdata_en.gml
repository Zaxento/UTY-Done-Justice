ds_map_add(global.text_data_en, "SCR_TEXT_3268", "\E0WELL^1, WORRY&NOT DEAR HUMAN!/")
ds_map_add(global.text_data_en, "SCR_TEXT_3269", "PAPYRUS WOULD&NEVER BETRAY YOU!/")
