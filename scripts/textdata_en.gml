ds_map_add(global.text_data_en, "SCR_TEXT_1934", "* If you need anything^1,&  just ask./%%")
ds_map_add(global.text_data_en, "SCR_TEXT_1935", "\E1* ... I have to do&  something.&* Stay here./%%.")
ds_map_add(global.text_data_en, "SCR_TEXT_1936", " ")
ds_map_add(global.text_data_en, "SCR_TEXT_1940", "\E2NYOO HOO HOO.../")
ds_map_add(global.text_data_en, "SCR_TEXT_1941", "I CAN'T EVEN&STOP SOMEONE AS&WEAK AS YOU.../")
ds_map_add(global.text_data_en, "SCR_TEXT_1942", "UNDYNE'S GOING TO&BE DISAPPOINTED&IN ME./")
ds_map_add(global.text_data_en, "SCR_TEXT_1943", "I'LL NEVER JOIN THE&ROYAL GUARD..^1.&AND.../")
ds_map_add(global.text_data_en, "SCR_TEXT_1944", "MY FRIEND QUANTITY&WILL REMAIN&STAGNANT!/")
ds_map_add(global.text_data_en, "SCR_TEXT_1945", "\TS \F0 \T0 %")
ds_map_add(global.text_data_en, "SCR_TEXT_1946", "* (What should you say?)&         Let's be    What a&         friends     loser\C")
ds_map_add(global.text_data_en, "SCR_TEXT_1947", " ")
ds_map_add(global.text_data_en, "SCR_TEXT_1956", "\E0WOWIE!^1!&YOU DID IT!!!/")
ds_map_add(global.text_data_en, "SCR_TEXT_1957", "YOU DIDN'T DO A&VIOLENCE!!!/")
ds_map_add(global.text_data_en, "SCR_TEXT_1958", "\E5TO BE HONEST^1,&I WAS A LITTLE&AFRAID.../")
ds_map_add(global.text_data_en, "SCR_TEXT_1959", "\E0BUT YOU'RE ALREADY&BECOMING A GREAT&PERSON!/")
ds_map_add(global.text_data_en, "SCR_TEXT_1960", "\E7I'M SO PROUD I&COULD CRY!!!/")
ds_map_add(global.text_data_en, "SCR_TEXT_1961", "\E3... WAIT^1, WASN'T I&SUPPOSED TO&CAPTURE YOU...?/")
ds_map_add(global.text_data_en, "SCR_TEXT_1962", "\E0WELL^1, FORGET IT!/")
ds_map_add(global.text_data_en, "SCR_TEXT_1963", "I JUST WANT YOU&TO BE THE BEST&PERSON YOU CAN BE./")
ds_map_add(global.text_data_en, "SCR_TEXT_1964", "SO LET'S LET&BYBONES BE&BYBONES./")
ds_map_add(global.text_data_en, "SCR_TEXT_1965", "I'LL EVEN TELL YOU&HOW TO LEAVE THE&UNDERGROUND!/")
ds_map_add(global.text_data_en, "SCR_TEXT_1966", "JUST KEEP GOING&EAST!/")
ds_map_add(global.text_data_en, "SCR_TEXT_1967", "EVENTUALLY YOU'LL&REACH THE KING'S&CASTLE./")
ds_map_add(global.text_data_en, "SCR_TEXT_1968", "THEN YOU CAN&LEAVE!/%%")
ds_map_add(global.text_data_en, "SCR_TEXT_1978", "REALLY!^1?&YOU WANT TO BE&FRIENDS^1, WITH ME???/")
ds_map_add(global.text_data_en, "SCR_TEXT_1979", "WELL THEN.../")
ds_map_add(global.text_data_en, "SCR_TEXT_1980", "\E0I GUESS..^1./")
ds_map_add(global.text_data_en, "SCR_TEXT_1981", "I GUESS&I CAN MAKE AN&ALLOWANCE FOR YOU!/")
ds_map_add(global.text_data_en, "SCR_TEXT_1982", " %")
ds_map_add(global.text_data_en, "SCR_TEXT_1987", "HUH^1?&WHY WOULD YOU.../")
ds_map_add(global.text_data_en, "SCR_TEXT_1988", "BERATE YOURSELF&SO LOUDLY???/")
ds_map_add(global.text_data_en, "SCR_TEXT_1989", "IS IT BECAUSE.../")
ds_map_add(global.text_data_en, "SCR_TEXT_1990", "\E3YOU DON'T THINK&YOU'RE GOOD ENOUGH&TO BE MY FRIEND?/")
ds_map_add(global.text_data_en, "SCR_TEXT_1991", "\E0NO!!^1!&YOU'RE GREAT!!^1!&I'LL BE YOUR FRIEND!/")
ds_map_add(global.text_data_en, "SCR_TEXT_1996", "WOWIE!^1!&WE HAVEN'T EVEN HAD&OUR FIRST DATE.../")
ds_map_add(global.text_data_en, "SCR_TEXT_1997", "AND I'VE ALREADY&MANAGED TO HIT&THE FRIEND ZONE!!!/")
ds_map_add(global.text_data_en, "SCR_TEXT_1998", "WHO KNEW THAT&ALL I NEEDED TO&MAKE PALS.../")
ds_map_add(global.text_data_en, "SCR_TEXT_2002", "WOW!!!/")
ds_map_add(global.text_data_en, "SCR_TEXT_2003", "I HAVE FRIENDS!!!/")
ds_map_add(global.text_data_en, "SCR_TEXT_2004", "AND WHO KNEW THAT&ALL I NEEDED TO&MAKE THEM.../")
ds_map_add(global.text_data_en, "SCR_TEXT_2006", "WAS TO GIVE PEOPLE&AWFUL PUZZLES AND&THEN FIGHT THEM??/")
ds_map_add(global.text_data_en, "SCR_TEXT_2007", "YOU TAUGHT ME A&LOT^1, HUMAN./")
ds_map_add(global.text_data_en, "SCR_TEXT_2008", "I HEREBY GRANT&YOU PERMISSION&TO PASS THROUGH!/")
ds_map_add(global.text_data_en, "SCR_TEXT_2009", "AND I'LL GIVE&YOU DIRECTIONS&TO THE SURFACE./")
ds_map_add(global.text_data_en, "SCR_TEXT_2010", "CONTINUE FORWARD&UNTIL YOU REACH THE&END OF THE CAVERN./")
ds_map_add(global.text_data_en, "SCR_TEXT_2011", "\WTHEN..^1. WHEN YOU&REACH THE CAPITAL^1,&CROSS \YTHE BARRIER\W./")
ds_map_add(global.text_data_en, "SCR_TEXT_2012", "THAT'S THE MAGICAL&SEAL TRAPPING US&ALL UNDERGROUND./")
ds_map_add(global.text_data_en, "SCR_TEXT_2013", "ANYTHING CAN ENTER&THROUGH IT^1, BUT&NOTHING CAN EXIT.../")
ds_map_add(global.text_data_en, "SCR_TEXT_2014", "... EXCEPT SOMEONE&WITH A POWERFUL&SOUL./")
ds_map_add(global.text_data_en, "SCR_TEXT_2015", "... LIKE YOU!!!/")
ds_map_add(global.text_data_en, "SCR_TEXT_2016", "THAT'S WHY THE&KING WANTS TO&ACQUIRE A HUMAN./")
ds_map_add(global.text_data_en, "SCR_TEXT_2017", "HE WANTS TO OPEN&THE BARRIER WITH&SOUL POWER./")
ds_map_add(global.text_data_en, "SCR_TEXT_2018", "THEN US MONSTERS&CAN RETURN TO&THE SURFACE!/%%")
ds_map_add(global.text_data_en, "SCR_TEXT_2023", "YOU'RE BACK AGAIN?!?!/")
ds_map_add(global.text_data_en, "SCR_TEXT_2024", "I FINALLY REALIZE&THE TRUE REASON WHY./")
ds_map_add(global.text_data_en, "SCR_TEXT_2025", "YOU.../")
ds_map_add(global.text_data_en, "SCR_TEXT_2026", "JUST MISS SEEING MY&FACE SO MUCH.../")
ds_map_add(global.text_data_en, "SCR_TEXT_2027", "I'M NOT SURE I&CAN FIGHT SOMEONE&WHO FEELS THIS WAY./")
ds_map_add(global.text_data_en, "SCR_TEXT_2028", "BUT MOSTLY..^1. I'M GETTING&REALLY TIRED OF&CAPTURING YOU!/")
ds_map_add(global.text_data_en, "SCR_TEXT_2029", "SO..^1.&WHAT DO YOU SAY?/")
ds_map_add(global.text_data_en, "SCR_TEXT_2030", "\TS \F0 \T0 %")
ds_map_add(global.text_data_en, "SCR_TEXT_2031", "* (Fight Papyrus?)& &         Yes         No\C")
