ds_map_add(global.text_data_en, "SCR_TEXT_3273", "SO, BECAUSE OF&MY INHERENT&GOODNESS.../")
ds_map_add(global.text_data_en, "SCR_TEXT_3274", "I TOLD HER YOU&WERE NOT WEARING&A \[1]!/")
