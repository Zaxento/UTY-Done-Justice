ds_map_add(global.text_data_en, "SCR_TEXT_3259", "NO BETRAYAL&ANYWHERE!!!/")
ds_map_add(global.text_data_en, "SCR_TEXT_3260", "BEING FRIENDS&WITH EVERYONE&IS EASY!!!/")
